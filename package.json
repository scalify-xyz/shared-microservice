{
  "name": "@scalify/shared-microservice",
  "version": "1.0.24",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "private": false,
  "files": [
    "build/**/*",
    "eslint.config.mjs",
    ".prettierrc",
    "tsconfig.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start:dev": "NODE_ENV=dev tsx watch src/index.ts",
    "start:local-build": "NODE_ENV=dev node build/index.js",
    "start:prod": "NODE_ENV=prod pm2 restart ecosystem.config.js --env production",
    "build": "tsc && tsc-alias --project tsconfig.json",
    "test": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "lint": "eslint .",
    "prettier": "npx prettier --check .",
    "prettier:fix": "npx prettier --write .",
    "lint:fix": "npm run lint --fix",
    "prepare": "husky",
    "commitlint": "commitlint --edit"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-typescript": "^7.26.0",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.16.0",
    "@types/amqplib": "^0.10.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.11.10",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "babel-jest": "^29.7.0",
    "commitizen": "^4.3.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-promise": "^6.6.0",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "ts-node": "10.9.1",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.17.0"
  },
  "dependencies": {
    "@aws-sdk/client-secrets-manager": "^3.716.0",
    "amqplib": "^0.10.5",
    "body-parser": "^1.19.1",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "reflect-metadata": "^0.1.13"
  }
}
